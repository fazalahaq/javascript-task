var color=["yellow","blue","red"];
if (color.includes("yellow")){ 
        document.write("yellow is my favColor")
}
else{
        document.write("yellow is not my favColor")
}


var col ="yellow";
for (var x in color) {
     col = color[x] + "yellow";
   }
